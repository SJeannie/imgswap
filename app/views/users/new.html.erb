
<%= flash[:error] %>

<%= form_with model: User.new do |f| %>
    <%= f.text_field :username, placeholder: 'create a username'%>
    <%= f.text_field :email, placeholder: 'email address'%>
    <%= f.password_field :password, placeholder: 'create a password'%>
    <%= f.password_field :password_confirmation, placeholder: 'retype your password'%>
    <%= f.submit %>
<% end %>